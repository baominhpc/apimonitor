<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd">


    <!-- A simple bean, with a simple name -->
    <bean id="myService" class="service.impl.MyServiceImpl"/>
    <bean id="testCaseService" class="service.impl.TestCaseServiceImpl"/>
    <bean id="parameterService" class="service.impl.ParameterServiceImpl"/>
    
    
    <bean id="testCaseDAO" class="models.dao.TestCaseDAO"/>
    <bean id="apiDAO" class="models.dao.APIDAO"/>
    <bean id="apiConfigDAO" class="models.dao.APIConfigDAO"/>
    <bean id="apiParameterDAO" class="models.dao.APIParameterDAO"/>
    <bean id="apiOperationDAO" class="models.dao.APIOperationDAO"/>
</beans>